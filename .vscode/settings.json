{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.rulers": [
    80
  ],
  "[typescript][javascript][json][markdown]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "files.exclude": {
    "out": false
  },
  "search.exclude": {
    "out": true
  },
  "typescript.tsc.autoDetect": "off",
  "denoHTMLTools.lint": true,
  "denoHTMLTools.lintOnSave": true,
  "deno.enable": false
}
