{
  "name": "vscode-deno-html-tools",
  "displayName": "deno-html-tools",
  "repository": {
    "url": "https://github.com/galiazzi/vscode-deno-html-tools"
  },
  "description": "",
  "version": "0.0.3",
  "publisher": "dayan",
  "icon": "icon.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:html"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
